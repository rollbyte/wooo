{
	"name" : "rollbyte/wooo",
	"minimum-stability" : "dev",
	"require-dev" : {
		"squizlabs/php_codesniffer" : "~3",
		"sebastian/phpcpd" : "5.0.1",
		"phpunit/phpunit" : "~7.5"
	},
	"require" : {
		"php": "^7.3",
		"psr/container" : "~1.0"
	},
	"autoload" : {
		"psr-4" : {
			"wooo\\core\\" : "core",
			"wooo\\lib\\" : "lib"
		},
		"files" : [
			"autoload.php"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"wooo\\tests\\" : "tests"
		}
	}
}